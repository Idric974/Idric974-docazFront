{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src/"
  },
  "include": [
    "src",
    "types",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/CompFilActualite.tsx",
    "pages/PosterArticle.js",
    "src/components/PosterArticle.js",
    "pages/index.js",
    "pages/_app.js",
    "src/utils/.js"
  ],
  "exclude": ["node_modules"]
}
